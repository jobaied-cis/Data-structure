#include <stdio.h>

int main() {
    int rows = 3, cols = 3;
    int arr[3][3];  // 2D array declaration

    // Input elements
    printf("Enter elements of a 3x3 matrix:\n");
    for(int i = 0; i < rows; i++) {
        for(int j = 0; j < cols; j++) {
            printf("Element at [%d][%d]: ", i, j);
            scanf("%d", &arr[i][j]);
        }
    }

    // Output elements
    printf("\nThe 3x3 matrix is:\n");
    for(int i = 0; i < rows; i++) {
        for(int j = 0; j < cols; j++) {
            printf("%d\t", arr[i][j]);
        }
        printf("\n");
    }

    return 0;
}
